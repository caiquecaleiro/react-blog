{
  "env": {
    "browser": true,
    "es6": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"installedESLint": true,
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"quotes": [
		"error",
		"single"
	],
	"semi": [
		"error",
		"always"
	]
}